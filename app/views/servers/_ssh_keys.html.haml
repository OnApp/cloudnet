- if ENV['DOCKER_PROVISIONER'].present? && !@server
  .jg-widget-content
    .jg-widget-header.pure-g.clearfix
      %h2.jg-icon.icon-cloud.pure-u Add SSH Keys
    .jg-widget-body
      .jg-widget-form.pure-u-1.clearfix
        .form-group.jg-inline.pure-u-1.pure-g
          #magic-checkbox-collection
            - current_user.keys.each do |key|
              .form-group.pure-u-md-1-4.magic-checkbox-item
                = check_box_tag :ssh_key_ids, key.id, false, name: 'server_wizard[ssh_key_ids][]', id: "ssh_key_ids_#{key.id}", class: 'magic-checkbox'
                = label_tag "ssh_key_ids_#{key.id}", key.title, "class" => "bs-tooltip", "data-toggle" => "tooltip", "data-placement" => "top", "data-container" => "body", "title" => key.key
